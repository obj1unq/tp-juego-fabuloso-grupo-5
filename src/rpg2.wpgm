import wollok.game.*
import starting.*
import battle.*
import teams2.*


program rpg {

	game.title("The Best RPG")
	game.cellSize(50)
	game.height(15)
	game.width(30)
	game.addVisual(startScreen)
	
	keyboard.enter().onPressDo({rules.show()})
	keyboard.backspace().onPressDo({game.stop()})
	
	game.start()

}